<div>
    {{#each products}}

    <div>
        <h3> {{title}} </h3>
            <img src=" {{thumbnail}} " alt="imagen de {{title}} ">
            <div>
                <p> {{description}} </p>
                <p>Precio: {{price}} </p>
                <p>Stock: {{stock}} </p>
                <p>Codigo: {{code}} </p> 
            </div>
    </div>
    {{/each}}
</div>